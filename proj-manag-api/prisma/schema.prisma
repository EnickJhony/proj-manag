// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id          Int       @id @unique @default(autoincrement())
  name        String
  description String
  startDate   DateTime
  endDate     DateTime?
  members     String[]
}